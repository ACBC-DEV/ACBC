{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@svg/*": ["./src/svg/*"],
      "@utils/*": ["./src/utils/*"],
      "@consts/*": ["./src/consts/*"]
    }
  }
}
